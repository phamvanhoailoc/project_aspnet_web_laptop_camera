<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Home - Zeiss Template</title>

    <!-- Main Styles -->
    <link rel="stylesheet" href="~/assets/admin/styles/style.min.css">

    <!-- Material Design Icon -->
    <link rel="stylesheet" href="~/assets/admin/fonts/material-design/css/materialdesignicons.css">

    <!-- mCustomScrollbar -->
    <link rel="stylesheet" href="~/assets/admin/plugin/mCustomScrollbar/jquery.mCustomScrollbar.min.css">

    <!-- Waves Effect -->
    <link rel="stylesheet" href="~/assets/admin/plugin/waves/waves.min.css">

    <!-- Sweet Alert -->
    <link rel="stylesheet" href="~/assets/admin/plugin/sweet-alert/sweetalert.css">

    <!-- Percent Circle -->
    <link rel="stylesheet" href="~/assets/admin/plugin/percircle/css/percircle.css">

    <!-- Chartist Chart -->
    <link rel="stylesheet" href="~/assets/admin/plugin/chart/chartist/chartist.min.css">

    <!-- FullCalendar -->
    <link rel="stylesheet" href="~/assets/admin/plugin/fullcalendar/fullcalendar.min.css">
    <link rel="stylesheet" href="~/assets/admin/plugin/fullcalendar/fullcalendar.print.css" media='print'>

    <!-- Dark Themes -->
    <link rel="stylesheet" href="~/assets/admin/styles/style-dark.min.css">
    @await RenderSectionAsync("Styles", required: false)

</head>

<body>

    @RenderBody()
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="~/assets/admin/script/html5shiv.min.js"></script>
        <script src="~/assets/admin/script/respond.min.js"></script>
    <![endif]-->
    <!--
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/assets/admin/scripts/jquery.min.js"></script>
    <script src="~/assets/admin/scripts/modernizr.min.js"></script>
    <script src="~/assets/admin/plugin/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/admin/plugin/mCustomScrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/assets/admin/plugin/nprogress/nprogress.js"></script>
    <script src="~/assets/admin/plugin/sweet-alert/sweetalert.min.js"></script>
    <script src="~/assets/admin/plugin/waves/waves.min.js"></script>
    <!-- Full Screen Plugin -->
    <script src="~/assets/admin/plugin/fullscreen/jquery.fullscreen-min.js"></script>

    <!-- Google Chart -->
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    <!-- chart.js Chart -->
    <script src="~/assets/admin/plugin/chart/chartjs/Chart.bundle.min.js"></script>
    <script src="~/assets/admin/scripts/chart.chartjs.init.min.js"></script>

    <!-- FullCalendar -->
    <script src="~/assets/admin/plugin/moment/moment.js"></script>
    <script src="~/assets/admin/plugin/fullcalendar/fullcalendar.min.js"></script>
    <script src="~/assets/admin/scripts/fullcalendar.init.js"></script>

    <script src="~/assets/admin/plugin/form-wizard/prettify.js"></script>
    <script src="~/assets/admin/plugin/form-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/assets/admin/scripts/form.wizard.init.min.js"></script>
    <!-- Sparkline Chart -->
    <script src="~/assets/admin/plugin/chart/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/assets/admin/scripts/chart.sparkline.init.min.js"></script>

    <script src="~/assets/admin/scripts/main.min.js"></script>
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>